@model IEnumerable<Expense_Tracker.Models.ExampleObject>

<h2>Objects</h2>
<a asp-action="Create">Add New Object</a>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Quantity</th>
            <th>Price</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.Quantity</td>
                <td>@obj.Price</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@obj.Id">Edit</a>
                    <a asp-action="Delete" asp-route-id="@obj.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
